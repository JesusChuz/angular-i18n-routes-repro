{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/en/index.html/"
    }, 
    {
      "route": "/index.html",
      "rewrite": "/en/index.html/"
    },  
    {
      "route": "/en",
      "rewrite": "/en/index.html/"
    }, 
    {
      "route": "/es",
      "rewrite": "/es/index.html/"
    },      
    {
      "route": "/testroute",
      "rewrite": "/testroute/index.html/"
    },      
    {
      "route": "/en/*.{js,css}",
      "rewrite": "/en/index.html/"
    }
  ],
  "navigationFallback": {
    "rewrite": "/en/index.html",
    "exclude": ["/assets/*", "/es/*.{js,css}", "/en/*.{js,css}"]
  }
}
