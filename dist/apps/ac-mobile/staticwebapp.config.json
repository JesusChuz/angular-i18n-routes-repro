{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/en/index.html/"
    },    
    {
      "route": "/index.html",
      "rewrite": "/en/index.html/"
    },
    {
      "route": "/testroute",
      "rewrite": "/testroute/index.html/"
    },
    {
      "route": "/es/profile",
      "rewrite": "/es/profile/index.html/"
    },
    {
      "route": "/es/*",
      "rewrite": "/es/index.html/"
    },
    {
      "route": "/en/*",
      "rewrite": "/"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/js/*"]
  }
}
