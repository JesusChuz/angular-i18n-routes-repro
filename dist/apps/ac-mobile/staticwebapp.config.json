{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/en/index.html"
    },
    {
      "route": "/en",
      "rewrite": "/en/index.html"
    },
    {
      "route": "/es",
      "rewrite": "/es/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/es/index.html",
    "exclude": ["/assets/*", "/es/*.{js,css}", "/en/*.{js,css}"]
  }
}
