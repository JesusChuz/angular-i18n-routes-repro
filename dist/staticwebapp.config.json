{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/dist/apps/ac-mobile/en/index.html"
    },
    {
      "route": "/index.html",
      "rewrite": "/dist/apps/ac-mobile/en/index.html"
    },
    {
      "route": "/en/*",
      "rewrite": "/dist/apps/ac-mobile/en/index.html"
    },
    {
      "route": "/es/*",
      "rewrite": "/dist/apps/ac-mobile/en/index.html"
    },
    {
      "route": "/testroute",
      "rewrite": "/dist/apps/ac-mobile/testroute/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/js/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
