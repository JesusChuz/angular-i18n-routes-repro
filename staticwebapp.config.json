{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "rewrite": "/testroute/index.html"
    },
    {
      "route": "/index.html",
      "rewrite": "/en/index.html"
    },
    {
      "route": "/en/*",
      "rewrite": "/en/index.html"
    },
    {
      "route": "/es/*",
      "rewrite": "/es/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/js/*"]
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self'"
  }
}
